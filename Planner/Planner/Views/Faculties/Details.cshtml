@model Domain.Models.Faculty

@{
    ViewBag.Title = "Детальніше";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<div class="content-wrapper " style="height: 900px!important; overflow-y: scroll;">
    <div style="width:600px; margin: 0 auto;">
        <p style="font-size: 24px!important" class="login-box-msg">Детальніше</p>
        <br />
    </div>

    <div>
        <h4>Список факультетів</h4>
        <hr />
        <dl class="dl-horizontal">
            <dt>
                <p style="font-size: 12px!important" class="login-box-msg">Повна назва</p>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.Name)
            </dd>

            <dt>
                <p style="font-size: 12px!important" class="login-box-msg">Коротка назва</p>
            </dt>

            <dd>
                @Html.DisplayFor(model => model.ShortName)
            </dd>

        </dl>
    </div>
    <p>
        @Html.ActionLink("Повернутися до списку", "Index", new { }, new { @style = "font-size: 16px!important", @class = "btn btn-primary" })
    </p>
</div>
